activate='source .venv/bin/activate'
alacritty-config='open ~/.config/alacritty/alacritty.yml'
apps-update='sudo nala update && sudo nala upgrade && flatpak update'
autoremove_apps='sudo apt-get autoremove'
cd1='cd ..'
cd2='cd ../..'
cd3='cd ../../..'
clear-shell='cat /dev/null > ~/.zsh_history && exit'
cls=clear
dlrs=download
ex=exit
hardware-specs='inxi -Fx'
history='atuin search -i'
home='cd ~'
install_app='sudo nala install'
l='exa -lB -a'
lgit='exa -lB --git'
ll='exa -lB'
ls=exa
lt='exa --tree --icons'
modelsim='cd /opt/intelFPGA/20.1/modelsim_ase/bin/  && sudo ./vsim'
remove_app='sudo apt-get remove'
rm=trash
starship-config='open ~/.config/starship.toml'
suspend-computer='sudo pm-suspend'
which-command=whence
zshconfig='open ~/.zshrc'
